# ship designer scale #
@cruiser_scale = 5
@belicose_scale = 5
@scythe_scale = 5
@stabber_scale = 5
@rupture_scale = 5

# in game size scale #
@cruiser_physical_scale = 1.0
@belicose_physical_scale = 1.0
@scythe_physical_scale = 1.0
@stabber_physical_scale = 1.0
@rupture_physical_scale = 1.0

@small_trail_W = 10
@small_trail_L = 60
