# reload assets gfx/models/ships/starbases/_starbase_caldari_entities.asset

# ship designer scale #
@pos_scale = 0.5
@shipyard_scale = 0.2
@refinery_scale = 1.0
@outpost_scale = 1.0
@starport_scale = 0.1
@starhold_scale = 1.0
@citadel_scale = 0.8


# in game size scale #
@shipyard_physical_scale = 2.0
@refinery_physical_scale = 2.0

@outpost_physical_scale = 0.2
@starport_physical_scale = 0.25
@starhold_physical_scale = 1
@citadel_physical_scale = 1


@jita_station_physical_scale = 0.5

@astrahus_frame_physical_scale = 0.3
@fortizar_frame_physical_scale = 1.5
@keepstar_frame_physical_scale = 1.0


